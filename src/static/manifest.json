{
  "name": "10000 hours",
  "description": "A google chrome extension to track your hours of mastery",
  "version": "1.0.3",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "10000 hours",
    "default_icon": "icon.png"
  },
  "permissions": ["storage", "unlimitedStorage", "alarms"],
  "optional_permissions": ["notifications"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
